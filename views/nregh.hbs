                  </div>
                  <div class="col-xs-4">
                     <label for="email">Email id</label>
                     <input type="email" class="form-control"  name="email" id="email" placeholder="Enter your email-id" required>
                  </div>
                 
                  <div class="col-xs-4">
                     <label for="password">Re-enter Password</label>
                     <input type="text" class="form-control" name="Add" id="Add" placeholder="Re-enter password" required>
                  </div>
                  <div class="col-xs-4">
                     <label for="example-date-input" >DOB</label>
                     <input class="form-control" type="date" name="dob" id="dob" value="2011-08-19" id="example-date-input">
                  </div>
                  <div class="col-xs-4">
                     </br>
                     <div class="text-center">
                   <input class="btn btn-primary text-center" type="submit" value="Register">
                     </div>
                  </div>
               </div>
            </form>
