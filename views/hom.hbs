                  </div>
                  <div class="col-xs-4">
                     <label for="exampleFormControlSelect1">Occupants</label>
                     <select class="form-control" id="exampleFormControlSelect1">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                     </select>
                  </div>
                  <div class="col-xs-4">
                     <label for="exampleFormControlSelect1">Room</label>
                     <select class="form-control" id="exampleFormControlSelect1">
                        <option>With Bathroom</option>
                        <option>Without Bathroom</option>
                     </select>
                  </div>
                  <div class="col-xs-4">
                     <label for="exampleFormControlSelect1">Bedding Type</label>
                     <select class="form-control" id="exampleFormControlSelect1">
                        <option>Single Bed</option>
                        <option>Double Bed</option>
                     </select>
                  </div>
                  </br>
                  <div class="text-center">
                     <input class="btn btn-primary text-center" type="button" value="Book">
                  </div>
               </div>
            </form>
